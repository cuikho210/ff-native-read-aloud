{
  "manifest_version": 2,
  "name": "Native Read Aloud",
  "version": "1.0",
  "description": "A tool to read aloud content using native tts",
  "icons": {
    "48": "icon-48.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ]
}
